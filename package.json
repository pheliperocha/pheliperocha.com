{
  "name": "pheliperocha",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test:coverage": "nyc npm run test:unit",
    "codacy": "nyc report --reporter=text-lcov | codacy-coverage --accountToken <accountToken> --username pheliperocha --projectName pheliperocha.com",
    "ci": "npm run lint && npm run test:unit && npm run test:e2e && npm run build",
    "lh": "lighthousebot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run ci"
    }
  },
  "dependencies": {
    "bootstrap-vue": "^2.1.0",
    "codacy-coverage": "^3.4.0",
    "flatpickr": "^4.6.3",
    "mocha-lcov-reporter": "1.3.0",
    "nouislider": "^14.1.1",
    "register-service-worker": "^1.0.0",
    "vue": "^2.6.6",
    "vue-class-component": "^7.1.0",
    "vue-flatpickr-component": "^8.1.5",
    "vue-i18n": "^8.15.3",
    "vue-lazyload": "^1.3.3",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.17.1",
    "vue2-transitions": "^0.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-plugin-unit-mocha": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "chai": "^4.1.2",
    "husky": "^2.3.0",
    "lighthousebot": "git+https://github.com/GoogleChromeLabs/lighthousebot.git",
    "node-sass": "^4.13.0",
    "nyc": "15.0.0",
    "sass-loader": "^7.1.0",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.6.6"
  }
}
